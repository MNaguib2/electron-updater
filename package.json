{
  "name": "electron-updater",
  "version": "1.0.1",
  "description": "My test electron app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --exec electron .",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/MNaguib2/electron-updater",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "https://portal.etolv.net/",
    "productName": "electron-updater",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "author": "mena afefe",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.9",
    "electron-builder": "^23.6.0",
    "nodemon": "^2.0.19"
  },
  "dependencies": {
    "electron-updater": "^5.3.0"
  }
}
